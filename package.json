  },
  "homepage": "https://github.com/JCellura/Nique_Physique#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.4"
  },
  "engines": {
    "node": "9.3.0"
  }
}
